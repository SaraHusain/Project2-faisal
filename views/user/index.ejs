  <div class="logoUserIndex">
    <img class="logos" src="/uploads/Screenshot (4).png">
  </div>

  <div class="userIndexPage">
    <div class="userIndexPageCon">
  <h1 class="userH1">Users List</h1>
 
  <table class="table table-dark table-striped-columns">
    <tr>
        <th>Avatar</th>
        <th>Name</th>
        <th>Email Address</th>
        <th>Phone Number</th>
        <th>Role</th>
        <th>Delete</th>
        <th>Edit</th>
    </tr>
    <% users.forEach(function (user) { %>
        <tr>
          <td>
            <img src="<%= user.avatar %>" alt="User Avatar">
          </td>
            <td>
              <a href="/user/detail?id=<%=  user._id %>"> 
              <%= user.name %></a>
            </td>
    
            <td><%= user.email %></td>
            <td><%= user.phoneNum %></td>
            <td><%= user.role %></td>

           <td><a href="/user/delete?id=<%= user._id %>">Delete</a></td> 
           <td><a href="/user/edit?id=<%= user._id %>">Edit</a></td>
        </tr>
    <%}) %>
    </table>
  
</div></div>