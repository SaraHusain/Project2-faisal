<h1>Room List</h1>

<table class="table table-striped">
<tr>
<th>Room Number</th>
<th>Delete</th>
<th>Edit</th>
</tr>

<% rooms.forEach(room => { %><tr>
    <td><a href="/room/details?id=<%= room._id %>"><%= room.roomNum %></a></td>
    
      <td><a href="/room/delete?id=<%= room._id %>">Delete</a></td>
    <td><a href="/room/edit?id=<%= room._id %>">Edit</a></td>

  </tr>
<% }); %>


</table>