<div class="bookingCon">
  <div class="bookingIndexCon">
<h1 class="roomsIndexH1">Room List</h1>

<div class="roomsIndexTable">
<table class="table table-striped">
<tr>
<th>Room Number</th>
<th>Delete</th>
<th>Edit</th>
</tr>

<% rooms.forEach(room => { %><tr>
    <td><a href="/rooms/detail?id=<%= room._id %>"><%= room.roomNum %></a></td>
      <td><a href="/rooms/delete?id=<%= room._id %>" class="btn btn-primary">Delete</a></td>
    <td><a href="/rooms/edit?id=<%= room._id %>" class="btn btn-primary">Edit</a></td>

  </tr>
<% }); %>


</table>
</div>
</div>
</div>