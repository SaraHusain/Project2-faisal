<h1>Booking List</h1>
<table class="table table-striped">
<tr>
<th>Bookings</th>
<th>Checking In At</th>
<th>Checking Out At</th>
<th>Delete</th>
<th>Edit</th>
</tr>
<hr>
<% bookings.forEach(function(booking){%>
<tr>
<td><a href="/booking/detail?id=<%=booking._id %>">Review Details</a></td>
<td><%= dayjs(booking.checkIn).format('MM/DD/YYYY')  %></td>
<td><%= dayjs(booking.checkOut).format('MM/DD/YYYY')  %></td>
<td><a href="/booking/delete?id=<%=booking._id %>" class="btn btn-primary">Delete</a></td>
<td><a href="/booking/edit?id=<%=booking._id %>" class="btn btn-primary">Edit</a></td>
</tr>
<% }) %>
</table>